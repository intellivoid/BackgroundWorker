        {
            "required": true,
            "file": "Exceptions/WorkerException.php"
        },
        {
            "required": true,
            "file": "Exceptions/NoWorkersRunningException.php"
        },
        {
            "required": true,
            "file": "Utilities/Converter.php"
        },
        {
            "required": true,
            "file": "Worker.php"
        },
        {
            "required": true,
            "file": "Objects/WorkerStatisticsResults.php"
        },
        {
            "required": true,
            "file": "Objects/WorkerInstance.php"
        }
    ],
    "files": [
        "package.json",
        "background_worker.json"
