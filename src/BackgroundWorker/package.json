{
    "package": {
        "package_name": "net.intellivoid.background_worker",
        "name": "BackgroundWorker",
        "version": "1.0.0.1",
        "author": "Zi Xing Narrakas",
        "organization": "Intellivoid",
        "description": "Extends tasks to sub-processing to power-process large tasks in the background or in parallel",
