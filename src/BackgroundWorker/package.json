                "version": "latest",
                "source": "default@github/intellivoid/VerboseAdventure",
                "required": true
            },
            {
                "package": "net.intellivoid.proclib",
                "version": "latest",
                "source": "default@github/intellivoid/ProcLib",
                "required": true
            },
            {
                "package": "net.intellivoid.timerlib",
                "version": "latest",
                "source": "default@github/intellivoid/TimerLib",
                "required": true
            }
        ],
        "configuration": {
            "autoload_method": "generated_spl",
            "main": null,
            "post_installation": [],
            "pre_installation": []
